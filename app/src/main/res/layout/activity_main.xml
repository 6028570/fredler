<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#B4EBB8"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardView"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.526">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="140dp"
            android:layout_height="126dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="20dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/keuken"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_Keuken"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/keuken"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="DuplicateIds,TextSizeCheck" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#B4E9EB"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.965">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/garage"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_garage"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/garage"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="DuplicateIds,TextSizeCheck,VisualLintBounds" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#EBB4E2"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.081">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="144dp"
            android:layout_height="122dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/woonkamer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_woonkamer"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/woonkamer"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="TextSizeCheck" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView8"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#B4C5EB"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardView2"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.081">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="150dp"
            android:layout_height="104dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="35dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/slaapkamer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_slaapkamer"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/Slaapkamer"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="DuplicateIds,TextSizeCheck" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#EBB4B4"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.525">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="107dp"
            android:layout_height="125dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="20dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/toilet"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_toilet"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/toilet"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="DuplicateIds,TextSizeCheck" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="182dp"
        android:layout_height="178dp"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#C3A5FF"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.923"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.965">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/image_content_description"
            android:scaleType="centerCrop"
            android:src="@drawable/badkamer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_garage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/verdana"
            android:gravity="center"
            android:paddingTop="0dp"
            android:text="@string/badkamer"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            tools:ignore="DuplicateIds,TextSizeCheck" />

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="148dp"
        android:layout_height="120dp"
        android:contentDescription="@string/logo"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/settings"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="2dp"
        android:contentDescription="@string/image_content_description"
        android:scaleType="centerCrop"
        android:src="@drawable/settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />



</androidx.constraintlayout.widget.ConstraintLayout>

